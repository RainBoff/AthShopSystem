@model IEnumerable<Data_Model_P.Sklep>
@using asp_mvc_std_v2
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Index";
}


<p>
    <a asp-action="Create">Create New</a>

</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nazwa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Opis)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nazwa)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Opis)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a> |
                    <a asp-action="ProduktyWSklepie" asp-route-id="@item.Id">Produkty</a> |
                    <a asp-action="ZamowieniaWSklepie_Index" asp-route-id="@item.Id">Zamówienia</a>|
                    <a asp-action="ZamowieniaZestawiania" asp-route-id="@item.Id">Zestawienia</a>
                </td>
            </tr>
        }
    </tbody>
</table>
